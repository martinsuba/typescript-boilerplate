{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "Simple typescript boilerplate for any project.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "karma start karma.config.js",
    "build": "npm run clean && webpack --config ./build/webpack.config.js --mode=development",
    "build:production": "npm run clean && webpack --config ./build/webpack.config.js --mode=production",
    "lint": "eslint --ignore-path .eslintignore . --ext .js,.tsx,.ts",
    "clean": "del-cli dist"
  },
  "author": "Martin Suba",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "del-cli": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "mocha": "^6.2.0",
    "sinon": "^7.3.2",
    "typescript": "^3.5.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yargs": "^13.3.0"
  },
  "dependencies": {}
}
